    "lib": [
      "es2017"
    ],
    "baseUrl": ".",
    "types": [
      "vite/client",
      "element-plus/global",
      "unplugin-icons/types/vue"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
